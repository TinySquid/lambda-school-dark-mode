{
  "manifest_version": 2,
  "name": "Lambda TK Dark Mode",
  "version": "1.0.0",
  "description": "A Firefox extension that gives the Lambda School Training Kit a darkmode.",
  "icons": {
    "48": "icons/lambda-48.png",
    "96": "icons/lambda-96.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://learn.lambdaschool.com/*"
      ],
      "css": [
        "/css/lambda-dark.min.css"
      ]
    }
  ]
}